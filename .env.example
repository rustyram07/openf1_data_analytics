# Databricks Configuration
# Copy this file to .env and fill in your credentials
# DO NOT commit .env to git!

# Databricks Workspace URL
# Example: https://adb-1234567890.8.azuredatabricks.net
DATABRICKS_HOST=

# Databricks Personal Access Token
# Generate from: Settings → Developer → Access Tokens
DATABRICKS_TOKEN=

# SQL Warehouse HTTP Path
# Example: /sql/1.0/warehouses/abcd1234efgh5678
# Find in: SQL Warehouses → Your Warehouse → Connection Details
DATABRICKS_HTTP_PATH=

# Optional: Catalog Configuration
BRONZE_CATALOG=bronze_f1_data_analytics
STAGE_CATALOG=stage_f1_data_analytics
PROD_CATALOG=prod_f1_data_analytics

# Optional: Local Data Directory
LOCAL_DATA_DIR=./test_data
